<script type="text/javascript">
  //<![CDATA[
  $(document).ready(function () {
    if ($.cookie("survey_banner")) {
        $(".signup-banner").hide();
    }
    $("#signup_banner").click(function () {
        $.cookie('survey_banner', 'true', { expires: 100000, path: '/' });
        $(".signup-banner").slideUp();
    });
    $(".signup").click(function () {
        $.cookie('survey_banner', 'true', { expires: 100000, path: '/' });
        $(".signup-banner").hide();
    });

    var sticky = $('.signup-banner');
    var mobMin = 640;
    var stickyHeight = sticky.height();
    var stickyWidth;

    if (sticky.length > 0) {
        stickyWidth = sticky.width();           
        if (stickyWidth <= mobMin) {
            $('.signup-banner > p:nth-child(1)').html(function (index, text) {
                return text.replace('Start4Life.', 'Start4Life.<br/>');
            });
            stickyHeight = sticky.height();
        }
    }

    $(window).scroll(function () {
        var scroll = $(window).scrollTop();
        if (scroll >= 10) {
            sticky.addClass('fixed');
            sticky.css('height', stickyHeight);
            if (stickyWidth <= mobMin) {
                sticky.css('width', stickyWidth + 40);
                sticky.css('height', stickyHeight + 22);
            } else {
                sticky.css('width', stickyWidth);
            }
        } else {
            sticky.removeClass('fixed');
        }
    });
  });
  //]]>
</script>


